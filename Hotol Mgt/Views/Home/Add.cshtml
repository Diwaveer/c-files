@model Hotol_Mgt.Models.College

<form asp-action="Add">


    <div>
        <label asp-for="StudentID">StudentID</label>
        <input asp-for="StudentID" type="text" />
    </div><br />
    <div>
        <label asp-for="StudentName">StudentName</label>
        <input asp-for="StudentName" type="text" />
    </div><br />

    <div>
        <label asp-for="StudentEmail">StudentEmail</label>
        <input asp-for="StudentEmail" type="text" />
    </div><br />
    <div>
        <label asp-for="StudentPhone">StudentPhone</label>
        <input asp-for="StudentPhone" type="text" />
    </div><br />

    <button type="submit">SUBMIT</button>
    <button asp-action="Read">READ</button>
    @if (TempData["a"] != null)
    {
        <h2>@TempData["a"]</h2>
    }
</form>


